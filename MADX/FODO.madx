TITLE, 'My first madx run';

BEAM, PARTICLE=ELECTRON,PC=3.0;

D: DRIFT,L=1.0;
QF: QUADRUPOLE,L=0.5,K1=0.2;
QD: QUADRUPOLE,L=0.5,K1=-0.2;

MyLine: LINE=(QF,5*(D),QD,5*(D),QF);
USE, PERIOD=MyLine;

TWISS,SAVE,BETX=15.0,BETY=5.0;
PLOT,HAXIS=S, VAXIS=BETX,BETY;

MATCH, SEQUENCE=MyLine;
PLOT,HAXIS=S, VAXIS=BETX, BETY;

Value, TABLE(SUMM, Q1);
Value, TABLE(SUMM, Q2);
WRITE, TABLE=SUMM,FILE=print.dat;